<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Contato</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/lib/materialize/css/materialize.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>

<body>
    <div class="navbar-fixed">
        <nav>
            <div class="nav-wrapper light-blue accent-4">
                <a href="#" class="brand-logo center"> Contato</a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li><a asp-action="Index" asp-controller="Home">Inicío</a></li>
                    <li><a asp-action="Administrativo" asp-controller="Home">Administrativo</a></li>
                </ul>
            </div>
        </nav>
    </div>

    <main>
        <div class="container" style="padding-top: 3.5%">
            @RenderBody()
        </div>
    </main>

    <footer class="page-footer light-blue darken-3">
        <div class="footer-copyright">
            <div class="container">
                © <cache>@DateTime.Now.Year</cache> - Sistema de contato
                <a class="grey-text text-lighten-4 right" target="_blank" href="http://cristianoprogramador.com">Desenvolvido por Cristiano Raffi Cunha</a>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/materialize/js/materialize.min.js"></script>
</body>

</html>